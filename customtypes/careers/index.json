{
  "format": "page",
  "id": "careers",
  "label": "Careers",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "uid": { "type": "UID", "config": { "label": "UID" } },
      "slice_zone": {
        "type": "Slices",
        "config": {
          "fieldset": "Careers Sections",
          "choices": {
            "job_listing": {
              "type": "Slice",
              "fieldset": "Job Listing",
              "icon": "briefcase",
              "variations": {
                "default": {
                  "primary": {
                    "position": { "type": "Text", "config": { "label": "Position Title" } },
                    "description": {
                      "type": "RichText",
                      "config": {
                        "label": "Job Description",
                        "multi": "paragraph, strong, em, hyperlink, list-item, o-list-item",
                        "allowTargetBlank": true
                      }
                    },
                    "apply_link": { "type": "Link", "config": { "label": "Apply Link" } }
                  }
                }
              }
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": { "type": "Text", "config": { "label": "Meta Title" } },
      "meta_description": { "type": "Text", "config": { "label": "Meta Description" } },
      "meta_image": { "type": "Image", "config": { "label": "Meta Image", "constraint": { "width": 2400, "height": 1260 }, "thumbnails": [] } }
    }
  }
}
